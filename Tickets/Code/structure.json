    "query": {
        "from": {
            "table": "Tickets\\Tickets\\Code\\Tables\\Tickets",
            "alias": "tt",
            "name": "#__tickets_tickets"
        },
        "select": [
            "tt.id",
            "tt.title",
            "tt.category_id",
            "tc.title AS category_id_title",
            "tt.department_id",
            "td.title AS department_id_title",
            "tt.assigned_to",
            "tt_8863.name AS assigned_to_name",
            "tt.name",
            "tt.username",
            "tt.email",
            "tt.phone",
            "tt.attachment",
            "mm.title AS attachment_title",
            "mm.file AS attachment_file",
            "tt.description",
            "tt.website",
            "tt.is_closed",
            "tt.created_by",
            "uu.name AS created_by_name",
            "tt.date_created",
            "tt.modified_by",
            "uu_5778.name AS modified_by_name",
            "tt.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Tickets\\Categories\\Code\\Tables\\Categories",
                "alias": "tc",
                "name": "#__tickets_categories",
                "on": "tt.category_id = tc.id"
            },
            {
                "table": "Tickets\\Departments\\Code\\Tables\\Departments",
                "alias": "td",
                "name": "#__tickets_departments",
                "on": "tt.department_id = td.id"
            },
            {
                "table": "Tickets\\Teams\\Code\\Tables\\Teams",
                "alias": "tt_8863",
                "name": "#__tickets_teams",
                "on": "tt.assigned_to = tt_8863.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "tt.attachment = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "tt.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5778",
                "name": "#__users_users",
                "on": "tt.modified_by = uu_5778.id"
            }
        ]
    }
