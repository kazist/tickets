    "query": {
        "from": {
            "table": "Tickets\\Tickets\\Comments\\Code\\Tables\\Comments",
            "alias": "ttc",
            "name": "#__tickets_tickets_comments"
        },
        "select": [
            "ttc.id",
            "ttc.comment",
            "ttc.ticket_id",
            "tt.title AS ticket_id_title",
            "ttc.attachment",
            "mm.title AS attachment_title",
            "mm.file AS attachment_file",
            "ttc.is_support",
            "ttc.date_created",
            "ttc.date_modified",
            "ttc.modified_by",
            "uu.name AS modified_by_name",
            "ttc.created_by",
            "uu_3074.name AS created_by_name"
        ],
        "leftjoin": [
            {
                "table": "Tickets\\Tickets\\Code\\Tables\\Tickets",
                "alias": "tt",
                "name": "#__tickets_tickets",
                "on": "ttc.ticket_id = tt.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "ttc.attachment = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ttc.modified_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_3074",
                "name": "#__users_users",
                "on": "ttc.created_by = uu_3074.id"
            }
        ]
    }
